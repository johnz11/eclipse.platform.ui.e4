<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.eclipse.e4.workbench.contributions" name="UI Contributions" schema="schema/org.eclipse.e4.workbench.contribution.exsd"/>
   <extension-point id="org.eclipse.e4.workbench.legacy" name="3.X legacy hook" schema="schema/org.eclipse.e4.workbench.legacy.exsd"/>
   <extension-point id="org.eclipse.e4.workbench.parts" name="e4 parts" schema="schema/org.eclipse.e4.workbench.parts.exsd"/>
   <extension-point id="org.eclipse.e4.workbench.partfactory" name="Part Factory" schema="schema/org.eclipse.e4.workbench.partfactory.exsd"/>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.core.runtime.IConfigurationElement"
            class="org.eclipse.e4.workbench.modeling.ICEAdapter">
         <adapter
               type="org.eclipse.e4.workbench.modeling.ModelHandlerBase">
         </adapter>
      </factory>
      <factory
            adaptableType="org.eclipse.emf.ecore.EObject"
            class="org.eclipse.e4.workbench.modeling.EObjModelHandler">
         <adapter
               type="org.eclipse.e4.workbench.modeling.ModelHandlerBase">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.e4.services">
      <servicefactory
            class="org.eclipse.e4.workbench.ui.SelectionServiceValue">
         <service
               api="org.eclipse.e4.ui.services.ISelectionService">
         </service>
      </servicefactory>
      <servicefactory
            class="org.eclipse.e4.workbench.ui.BackgroundRunnerValue">
         <service
               api="org.eclipse.e4.core.services.IBackgroundRunner">
         </service>
      </servicefactory>
      <servicefactory
            class="org.eclipse.e4.workbench.ui.ProgressMonitorValue">
         <service
               api="org.eclipse.core.runtime.IProgressMonitor">
         </service>
      </servicefactory>
   </extension>
   <extension
         point="org.eclipse.e4.workbench.partfactory">
      <factory
            class="org.eclipse.e4.workbench.ui.renderers.swt.CompositePartFactory">
      </factory>
   </extension>
   <extension
         point="org.eclipse.e4.workbench.partfactory">
      <factory
            class="org.eclipse.e4.workbench.ui.renderers.swt.PartSashFactory">
      </factory>
   </extension>
   <extension
         point="org.eclipse.e4.workbench.partfactory">
      <factory
            class="org.eclipse.e4.workbench.ui.renderers.swt.WBPartFactory">
      </factory>
   </extension>
   <extension
         point="org.eclipse.e4.workbench.partfactory">
      <factory
            class="org.eclipse.e4.workbench.ui.renderers.swt.ContributedPartFactory">
      </factory>
   </extension>
   <extension
         point="org.eclipse.e4.workbench.partfactory">
      <factory
            class="org.eclipse.e4.workbench.ui.renderers.swt.StackModelFactory">
      </factory>
   </extension>
   <extension
         id="application"
         name="Workbench Application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.e4.workbench.ui.internal.WorkbenchApplication">
         </run>
      </application>
   </extension>

</plugin>
