<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.e4.ui.css.swt.property.handler">
      <handler
            adapter="org.eclipse.e4.ui.css.swt.dom.CompositeElement"
            composite="false"
            handler="org.eclipse.e4.ui.workbench.addons.perspectiveswitcher.PerspectiveSwitcherCSSHandler">
         <property-name
               name="perspective-keyline-color">
         </property-name>
      </handler>
   </extension>
   <extension
         id="AddonSnippet"
         name="Addon Snippet"
         point="org.eclipse.e4.workbench.model">
      <processor
            beforefragment="true"
            class="org.eclipse.e4.ui.workbench.addons.swt.CleanupProcessor">
      </processor>
      <processor
            beforefragment="true"
            class="org.eclipse.e4.ui.workbench.addons.swt.MinMaxProcessor">
      </processor>
      <processor
            beforefragment="true"
            class="org.eclipse.e4.ui.workbench.addons.swt.DnDProcessor">
      </processor>
   </extension>

</plugin>
