<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.e4.services">
      <servicefactory
            class="org.eclipse.e4.demo.e4photo.ServiceFactory">
         <service
               api="org.eclipse.core.resources.IWorkspace">
         </service>
         <service
               api="org.eclipse.core.databinding.observable.Realm">
         </service>
      </servicefactory>
   </extension>
   <extension
         point="org.eclipse.e4.workbench.parts">
      <part
            class="org.eclipse.e4.demo.e4photo.Library"
            label="Albums"
            parentId="org.eclipse.e4.ui.tags.navigation">
      </part>
      <part
            class="org.eclipse.e4.demo.e4photo.Thumbnails"
            label="Thumbnails"
            parentId="org.eclipse.e4.ui.tags.content">
      </part>
      <part
            class="org.eclipse.e4.demo.e4photo.Preview"
            label="Preview"
            parentId="org.eclipse.e4.ui.tags.details">
      </part>
      <part
            class="org.eclipse.e4.demo.e4photo.ExifTable"
            label="Details"
            parentId="org.eclipse.e4.ui.tags.content">
      </part>
      <part
            class="org.eclipse.e4.demo.e4photo.Location"
            label="Map"
            parentId="org.eclipse.e4.ui.tags.geolocation">
      </part>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.e4.demo.e4photo.Exif"
            class="org.eclipse.e4.demo.e4photo.AdapterFactory">
         <adapter
               type="org.eclipse.core.resources.IFile">
         </adapter>
      </factory>
   </extension>
   <extension
         id="app"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.e4.demo.e4photo.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.demo.e4photo.app"
            name="e4 photo demo">
      </product>
   </extension>

</plugin>
