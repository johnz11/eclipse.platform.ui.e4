<?xml version="1.0" encoding="ASCII"?>
<workbench:WorkbenchWindow xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:workbench="http://www.eclipse.org/e4/workbench" xsi:schemaLocation="http://www.eclipse.org/e4/workbench ../org.eclipse.e4.workbench.emf/model/Workbench.ecore">
  <handlers plugin="org.eclipse.e4.demo.e4photo" classname="org.eclipse.e4.demo.e4photo.NewProjectHandler"/>
  <handlers plugin="org.eclipse.e4.demo.e4photo" classname="org.eclipse.e4.demo.e4photo.ExitHandler"/>
  <handlers plugin="org.eclipse.e4.demo.e4photo" classname="org.eclipse.e4.demo.e4photo.DeleteProjectHandler"/>
  <mainMenu label="main">
    <children xsi:type="workbench:MenuModel" label="File">
      <children xsi:type="workbench:ItemModel" label="New Album" handler="//@handlers.0"/>
      <children xsi:type="workbench:ItemModel" separator="true"/>
      <children xsi:type="workbench:ItemModel" label="Exit" iconPath="" handler="//@handlers.1"/>
    </children>
    <children xsi:type="workbench:MenuModel" label="Edit">
      <children xsi:type="workbench:ItemModel" label="Delete Album" handler="//@handlers.2"/>
    </children>
  </mainMenu>
  <perspectives id="perspectiveId" label="perspectiveLabel" toolTip="perspectiveTooltip">
    <children xsi:type="workbench:PartSash" horizontal="true">
      <children xsi:type="workbench:Composite">
        <children xsi:type="workbench:ToolbarModel">
          <children xsi:type="workbench:ItemModel" label="New Album" handler="//@handlers.0"/>
          <children xsi:type="workbench:ItemModel" label="Delete Album" handler="//@handlers.2"/>
        </children>
        <children xsi:type="workbench:PartSash">
          <children xsi:type="workbench:StackModel">
            <children xsi:type="workbench:ContributedPart" plugin="org.eclipse.e4.demo.e4photo" label="Library" classname="org.eclipse.e4.demo.e4photo.Library"/>
          </children>
          <children xsi:type="workbench:StackModel">
            <children xsi:type="workbench:ContributedPart" plugin="org.eclipse.e4.demo.e4photo" label="Map" classname="org.eclipse.e4.demo.e4photo.Location"/>
          </children>
        </children>
      </children>
      <children xsi:type="workbench:PartSash">
        <children xsi:type="workbench:ContributedPart" plugin="org.eclipse.e4.demo.e4photo" label="Preview" classname="org.eclipse.e4.demo.e4photo.Preview"/>
        <children xsi:type="workbench:StackModel">
          <children xsi:type="workbench:ContributedPart" plugin="org.eclipse.e4.demo.e4photo" label="Thumbnails" classname="org.eclipse.e4.demo.e4photo.Thumbnails"/>
          <children xsi:type="workbench:ContributedPart" plugin="org.eclipse.e4.demo.e4photo" label="Exif" classname="org.eclipse.e4.demo.e4photo.ExifTable"/>
        </children>
        <weights>70</weights>
        <weights>30</weights>
      </children>
      <weights>25</weights>
      <weights>75</weights>
    </children>
  </perspectives>
</workbench:WorkbenchWindow>
